{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "quotes": 0,
    "comma-dangle": "off",
    "no-console": 0,
    "no-multiple-empty-lines": 0,
    "spaced-comment": 0,
    "operator-linebreak": "off",
    "strict": 0
  }
}
